{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ],
      "expo-localization"
    ],
    "name": "flatsharing",
    "android": {
      "softwareKeyboardLayoutMode": "pan"
    },
    "slug": "flatsharing",
    "orientation": "portrait"
  }
}
